							</ctlp:schemas>
						</ctlp:XMLValidatingParser>
					</ctlp:SOAPParser>
				</ctl:soap-request>
			</xsl:variable>
			<xsl:choose>
				<xsl:when test="empty($Capabilities)">
					<ctl:message>[FAILURE] Parsing or validation errors.</ctl:message>
					<ctl:fail/>
				</xsl:when>
				<xsl:otherwise>
					<xsl:if test="empty($Capabilities//wcs:Capabilities)">
