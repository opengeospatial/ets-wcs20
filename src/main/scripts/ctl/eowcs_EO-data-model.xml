			<xsl:for-each select="$Capabilities//*[local-name()='CoverageSummary'][*[local-name()='CoverageSubtype']='ReferenceableStitchedMosaic']/*[local-name()='CoverageId']">
				<xsl:variable name="describeeocoverage">
