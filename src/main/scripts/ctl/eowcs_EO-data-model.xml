				</xsl:if>
			</xsl:for-each>

			<xsl:if test="count($response//*[local-name()='DatasetSeriesDescription']//*[local-name()='DatasetSeriesId'])!=count(distinct-values(($response//*[local-name()='DatasetSeriesDescription']//*[local-name()='DatasetSeriesId'])))">
