						<ctl:param name="EOID">
							<xsl:value-of select="."/>
						</ctl:param>
						<ctlp:XMLValidatingParser>
							<ctlp:schemas>
								<ctlp:schema type="url">http://schemas.opengis.net/wcs/wcseo/1.0/wcsEODescribeEOCoverageSet.xsd</ctlp:schema>
							</ctlp:schemas>
						</ctlp:XMLValidatingParser>
